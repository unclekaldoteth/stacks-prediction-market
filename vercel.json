{
    "version": 2,
    "builds": [
        {
            "src": "frontend-pools/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/frontend-pools/$1"
        }
    ],
    "framework": "nextjs",
    "installCommand": "cd frontend-pools && npm install",
    "buildCommand": "cd frontend-pools && npm run build",
    "outputDirectory": "frontend-pools/.next"
}